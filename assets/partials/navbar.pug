- 
    const navLinks = [
        'home',
        'about',
        'portfolio',
        'services'
    ]


#navbar.h-20.w-full.fixed.top-0.z-50.hidden( class = 'md:block' )
    .my-container
        .justify-between.flex.h-20.m-auto.py-6
            h2.ml-8.text-2xl.title.text-white TD
            p.hidden.text-gray-500 ( Just so tailwind can see text-gray-500 )
            ul.flex.justify-around( class = 'w-2/5' )
                each navLink, index in navLinks
                    li( key = index )
                        a.mx-0.text-xs.text-white(
                            href  = `#${ navLink }`
                            class = 'hover:text-gray-500 sm:mx-2'
                            class = 'links'
                        )= navLink.toUpperCase()
